{
  "name": "kyranxx-diara-configurator",
  "version": "1.0.0",
  "description": "3D bracelet configurator",
  "main": "server/index.ts",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "webpack serve --mode development",
    "dev:backend": "ts-node-dev --respawn -P tsconfig.server.json server/index.ts",
    "prebuild": "node -e \"if (require('fs').existsSync('dist')) require('fs').rmSync('dist', { recursive: true })\"",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "webpack-build": "webpack --mode production",
    "vercel-build": "node vercel-build.js",
    "start": "ts-node -P tsconfig.server.json server/index.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@react-three/drei": "^9.88.17",
    "@react-three/fiber": "^8.15.11",
    "@shopify/shopify-api": "^7.0.0",
    "@shopify/shopify-app-express": "^2.0.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.0",
    "three": "^0.150.1",
    "three-mesh-bvh": "^0.5.23"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/eslint-parser": "^7.26.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.24.0",
    "@babel/plugin-transform-private-methods": "^7.24.0",
    "@babel/plugin-transform-private-property-in-object": "^7.24.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.26.0",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/sharp": "^0.31.1",
    "@types/three": "^0.150.1",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "html-webpack-plugin": "^5.5.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.2",
    "postcss-import": "^16.1.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.0",
    "style-loader": "^3.3.2",
    "tailwindcss": "^4.0.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "resolutions": {
    "lodash": "^4.17.21"
  },
  "overrides": {
    "lodash": "^4.17.21"
  }
}
